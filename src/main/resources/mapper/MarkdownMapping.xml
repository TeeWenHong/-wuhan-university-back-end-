<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qingshuge.dao.MarkdownMapper">
    <insert id="saveMarkdown" parameterType="Markdown">
        INSERT INTO chapters (book_path,book_id,chapters,title) VALUES (#{book_path},#{book_id},#{chapters},#{title})
    </insert>
</mapper>
